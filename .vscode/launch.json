    {
      "name": "Docker Node.js Launch",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "port": 9229,
      "restart": true,
      "sourceMaps": true,
      "smartStep": true,
      "address": "localhost",
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/opt/WickrIODebug/clients/bcstlocalbot/integration/wickrio-broadcast-bot",
      "timeout": 50000,
      "showAsyncStacks": true,
      "sourceMapPathOverrides": {
        "/opt/WickrIODebug/clients/bcstlocalbot/integration/wickrio-broadcast-bot/src/*": "${workspaceFolder}/src/*"
      },
      "skipFiles": [
        "/usr/src/app/node_modules/**/*.js",
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
